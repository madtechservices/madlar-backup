{
    "name": "tomatophp/tomato-backup",
    "type": "library",
    "description": "Backup plugin for TomatoPHP build with spatie laravel-backup",
    "keywords": [
        "php",
        "laravel",
        "backup",
        "laravel-backup",
        "spatie",
        "tomatophp",
        "plugin"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoBackup\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoBackup\\TomatoBackupServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.0",
        "tomatophp/tomato-admin": "^1.1",
        "tomatophp/tomato-roles": "^1.1",
        "spatie/laravel-backup": "^8.1",
        "calebporzio/sushi": "^2.4"
    }
}
